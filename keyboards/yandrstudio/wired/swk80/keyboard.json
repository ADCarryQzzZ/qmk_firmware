{
    "keyboard_name": "SWK80",
    "processor": "STM32F103",
    "bootloader": "uf2boot",
    "board": "STM32_F103_STM32DUINO",
    "usb": {
        "pid": "0xAADE",
        "device_version": "1.0.0"
    },
    "build": {
        "lto": true,
        "debounce_type": "custom",
        "firmware_format": "uf2"
    },
    "matrix_pins": {
        "cols": ["A5", "B10", "B11", "B2", "B1", "B0", "A7", "A6", "B14", "B15", "A8", "A9", "B13", "B12", "A15", "B3", "B4"],
        "rows": ["A0", "B6",  "B5",  "A10", "A2", "A1"]
    },
    "diode_direction": "COL2ROW",
    "qmk": {
        "tap_capslock_delay": 250,
        "tap_keycode_delay": 15
    },
    "features": {
        "bootmagic": true,
        "extrakey": true,
        "mousekey": true,
        "nkro": true,
        "rgb_matrix": true
    },
    "ws2812": {
        "pin": "A3",
        "driver": "pwm"
    },
    "rgb_matrix": {
        "animations": {
            "alphas_mods": true,
            "gradient_up_down": true,
            "gradient_left_right": true,
            "breathing": true,
            "band_sat": true,
            "band_val": true,
            "band_pinwheel_sat": true,
            "band_pinwheel_val": true,
            "band_spiral_sat": true,
            "band_spiral_val": true,
            "cycle_all": true,
            "cycle_left_right": true,
            "cycle_up_down": true,
            "rainbow_moving_chevron": true,
            "cycle_out_in": true,
            "cycle_out_in_dual": true,
            "cycle_pinwheel": true,
            "cycle_spiral": true,
            "dual_beacon": true,
            "rainbow_beacon": true,
            "rainbow_pinwheels": true,
            "raindrops": true,
            "jellybean_raindrops": true,
            "hue_breathing": true,
            "hue_pendulum": true,
            "hue_wave": true,
            "pixel_rain": true,
            "pixel_flow": true,
            "pixel_fractal": true,
            "typing_heatmap": true,
            "digital_rain": true,
            "solid_reactive_simple": true,
            "solid_reactive": true,
            "solid_reactive_wide": true,
            "solid_reactive_multiwide": true,
            "solid_reactive_cross": true,
            "solid_reactive_multicross": true,
            "solid_reactive_nexus": true,
            "solid_reactive_multinexus": true,
            "splash": true,
            "multisplash": true,
            "solid_splash": true,
            "solid_multisplash": true
        },
        "driver": "ws2812",
        "max_brightness": 180,
        "sleep": true
    }
}
